<?xml version="1.0" encoding="UTF-8"?>
<pepper-job id="trttqfob" version="1.0">
  <importer name="EXMARaLDAImporter" path="./tests/data/merge_test/" />
  <importer name="CoNLLImporter" path="./tests/data/merge_test/">
    <customization>
      <property key="text_name">conll</property>
      <property key="anno_ns">norm</property>
      <property key="skip_named_ordering">false</property>
    </customization>
  </importer>
  <importer name="import_ptb" path="./tests/data/merge_test/">
    <customization>
      <property key="text_name">ptb</property>
      <property key="anno_ns">topo</property>
      <property key="edge_layer">const</property>
    </customization>
  </importer>
  <manipulator name="merge">
    <customization>
      <property key="check.names">norm,conll,ptb</property>
      <property key="keep.name">norm</property>
      <property key="root.name">rueg</property>
      <property key="skip.components">,</property>
      <property key="silent">true</property>
    </customization>
  </manipulator>
  <manipulator name="replace">
    <customization>
      <property key="node.annos">norm::Number[psor],norm::Gender[psor]</property>
    </customization>
  </manipulator>
  <exporter name="export_graphml" path="./">
    <property key="guess.vis">true</property>
    <!--property
    key="add.vis">
[[visualizers]]
element = "node"
layer = ""
vis_type = "grid"
display_name = "annotations"
visibility = "hidden"

[visualizers.mappings]
annos = "/dipl::cu/"

[[visualizers]]
element = "edge"
layer = "dependencies"
vis_type = "arch_dependency"
display_name = "dependencies"
visibility = "hidden"

[visualizers.mappings]
node_key = "norm"
	  </property-->
  </exporter>
</pepper-job>