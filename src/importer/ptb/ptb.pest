WHITESPACE = _{ " " | "\t" | NEWLINE }

label_character = @{!("(" | ")" | "\"" | WHITESPACE) ~ ANY}
value_character = @{!"\"" ~ ANY}

label = @{label_character+}
quoted_value = @{"\"" ~ value_character* ~ "\""}

phrase = {"(" ~ label ~ (phrase+ | quoted_value | label) ~ ")"}
root = {"(" ~ phrase ~ ")"}

ptb = {SOI ~ (root+ | phrase) ~ EOI}